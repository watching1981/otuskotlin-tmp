{
  "request": {
    "method": "POST",
    "url": "/v1/ad/delete"
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "responseType": "delete",
      "result": "success",
      "deletedAdId": "{{{jsonPath request.body '$.id'}}}"
    },
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": ["response-template"]
  }
}
